<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Framework1" parallel="tests" thread-count="4">

        <listeners>
            <listener class-name="org.TestComponents.Listeners"/>
        </listeners>


    <groups>
        <run>
            <include name="endtoendtest"></include>
        </run>
    </groups>
    <test thread-count="5" name="StandAloneE2ETest">
        <classes>

            <class name="org.Tests.SubmitOrder"/>
        </classes>
    </test> <!-- Framework1 -->
    <test thread-count="5" name="ErrorValidation">
        <classes>
            <class name="org.Tests.ErrorValidation"/>

        </classes>
    </test> <!-- Framework1 -->
</suite> <!-- Default Suite -->
